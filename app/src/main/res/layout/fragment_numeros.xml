<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/segundo_fundo"
    tools:context=".Fragments.NumerosFragment">

    <ImageView
        android:id="@+id/imageUm"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:src="@drawable/um"
        app:layout_constraintBottom_toTopOf="@+id/imageTres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageDois"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/dois"
        app:layout_constraintBottom_toTopOf="@+id/imageQuatro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageUm"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageQuatro"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/quatro"
        app:layout_constraintBottom_toTopOf="@+id/imageSeis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageTres"
        app:layout_constraintTop_toBottomOf="@+id/imageDois" />

    <ImageView
        android:id="@+id/imageSeis"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/seis"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageCinco"
        app:layout_constraintTop_toBottomOf="@+id/imageQuatro" />

    <ImageView
        android:id="@+id/imageTres"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:src="@drawable/tres"
        app:layout_constraintBottom_toTopOf="@+id/imageCinco"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageUm" />

    <ImageView
        android:id="@+id/imageCinco"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="40dp"
        android:src="@drawable/cinco"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageTres" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.90997565" />

</androidx.constraintlayout.widget.ConstraintLayout>